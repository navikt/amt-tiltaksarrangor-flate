{
  "name": "amt-tiltaksarrangor-flate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:gh-pages": "NODE_ENV=development --mode demo vite build --base=/amt-tiltaksarrangor-flate/ && cpy --cwd=build --rename=404.html index.html .",
    "build:pull-request": "BASE=/deltakeroversikt/pr-${{ github.event.number }}/ --mode pull-request vite build",
    "test": "vitest",
    "lint": "eslint --max-warnings=0 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx,js}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:clear": "cypress cache clear"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write src/**/*"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@navikt/aksel-icons": "^7.9.1",
    "@navikt/ds-css": "^7.9.1",
